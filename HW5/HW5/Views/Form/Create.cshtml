
@{
    ViewBag.Title = "Create";
}

<h2>Create Request</h2>

<form method="post">
<div class="container">
    <div class="row">
        <div class="col-sm-4">V#:</div>
        <div class="col-sm-8"><input type="number" name="ID"></div>
    </div>
    <div class="row">
        <div class="col-sm-4">First Name:</div>
        <div class="col-sm-8"><input type="text" name="FirstName"></div>
    </div>
    <div class="row">
        <div class="col-sm-4">Last Name:</div>
        <div class="col-sm-8"><input type="text" name="LastName"></div>
    </div>
    <div class="row">
        <div class="col-sm-4">Date of Birth:</div>
        <div class="col-sm-8"><input type="date" name="DOB"></div>
    </div>
    <div class="row">
        <div class="col-sm-4">Catalog Year:</div>
        <div class="col-sm-8"><input type="number" name="C_year"></div>
    </div>
    <div class="row">
        <div class="col-sm-4">Date:</div>
        <div class="col-sm-8"><input type="date" name="Date"></div>
    </div>
    <div class="row">
        <div class="col-sm-4">Phone Number:</div>
        <div class="col-sm-8"><input type="tel" name="Phone"></div>
    </div>
    <div class="row">
        <div class="col-sm-4">Email:</div>
        <div class="col-sm-8"><input type="text" name="Email"></div>
    </div>
    <div class="row">
        <div class="col-sm-4">Major:</div>
        <div class="col-sm-8"><input type="text" name="Major"></div>
    </div>
    <div class="row">
        <div class="col-sm-4">Minor:</div>
        <div class="col-sm-8"><input type="text" name="Minor"></div>
    </div>
    <div class="row">
        <div class="col-sm-4">Advisor:</div>
        <div class="col-sm-8"><input type="text" name="Advisor"></div>
    </div>
    <div class="row">
        <div class="col-sm-4"><input type="submit" class="btn btn-default" value="Submit Request" name="submit" id="submit"></div>
        <div class="col-sm-8"></div>
    </div>
</div>
</form> 